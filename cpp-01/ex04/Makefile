CC = c++ -Wall -Wextra -Werror -std=c++98 -g -o a.out

SRC =  main.cpp 

NAME = a.out

all: $(NAME)

$(NAME): $(SRC)
	$(CC) $(SRC)

clean:
	rm -f $(NAME) 

fclean: clean	
	rm -f $(NAME) 

re : fclean all